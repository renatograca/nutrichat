# Example environment variables for NutriChat
# Copy to .env and fill with real values (do NOT commit secrets to git)

# Environment
ENV=production
PORT=8080

# Database (you can supply DATABASE_URL or the parts below)
# DATABASE_URL=postgresql://postgres:password@db:5432/nutri
DB_HOST=db
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=nutri

# AI providers
OPENAI_API_KEY=
GOOGLE_API_KEY=
